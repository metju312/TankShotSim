<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<objectModel xsi:schemaLocation="http://standards.ieee.org/IEEE1516-2010 http://standards.ieee.org/downloads/1516/1516.2-2010/IEEE1516-DIF-2010.xsd" xmlns="http://standards.ieee.org/IEEE1516-2010" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelIdentification>
        <name>TankSim</name>
        <type>FOM</type>
        <version>1.0</version>
        <modificationDate>2017-06-04</modificationDate>
        <securityClassification>Unclassified</securityClassification>
        <releaseRestriction>Other organizations not related to Federated Foods</releaseRestriction>
        <purpose>To define a  soup module for a restaurant federation</purpose>
        <applicationDomain>Restaurant operations</applicationDomain>
        <description>Soup for the modular restaurant.</description>
        <useLimitation>Derived models must reference this document</useLimitation>
        <useHistory>Originally used as an example in the DoD HLA 1.3 specification</useHistory>
        <useHistory>Used as an example in the HLA IEEE1516.2-2000 specification</useHistory>
        <useHistory>Used as an example in the HLA IEEE1516.2-2010 specification after conversion to a FOM module (MIM information stripped out)</useHistory>
        <keyword>
            <taxonomy>Food Service Industry Taxonomy</taxonomy>
            <keywordValue>Restaurant</keywordValue>
        </keyword>
        <keyword>
            <taxonomy>Food Service Industry Taxonomy</taxonomy>
            <keywordValue>Menu</keywordValue>
        </keyword>
        <keyword>
            <taxonomy>Food Service Industry Taxonomy</taxonomy>
            <keywordValue>Waiter</keywordValue>
        </keyword>
        <poc>
            <pocType>Sponsor</pocType>
            <pocName>Mr. Joseph Doe</pocName>
            <pocOrg>Joe's Place</pocOrg>
            <pocTelephone>1-977-555-1234</pocTelephone>
            <pocEmail>doej@fedfoods.com</pocEmail>
        </poc>
        <poc>
            <pocType>Author</pocType>
            <pocName>Mr. Sven Swede</pocName>
            <pocOrg>Federated Foods</pocOrg>
        </poc>
        <reference>
            <type>Dependency</type>
            <identification>TankSim</identification>
        </reference>
        <reference>
            <type>HTML document</type>
            <identification>www.fedfoods.com/restsim.html</identification>
        </reference>
        <reference>
            <type>Federation Agreements</type>
            <identification>www.fedfoods.com/restsimAgreement.pdf</identification>
        </reference>
        <other>See Mobil International Restaurant Guide for more information</other>
        <glyph alt="Restaurant" width="74" height="74" type="jpg"></glyph>
    </modelIdentification>
    <objects>
        <objectClass>
            <name>HLAobjectRoot</name>
            <objectClass>
				<name>Terrain</name>
				<sharing>PublishSubscribe</sharing>
				<semantics>Shape of the terrain</semantics>
				<attribute>
					<name>Shape</name>
					<dataType>Vector3</dataType>
					<updateType>Conditional</updateType>
					<updateCondition>Customer request</updateCondition>
					<ownership>NoTransfer</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>TimeStamp</order>
					<semantics>Change in tarrain</semantics>
				</attribute>
			</objectClass>

            <objectClass>
				<name>Atmosphere</name>
				<sharing>PublishSubscribe</sharing>
				<semantics>Affects the missile's flight</semantics>
				<attribute>
					<name>WindDirectory</name>
					<dataType>Vector3</dataType>
					<updateType>Conditional</updateType>
					<updateCondition>Customer request</updateCondition>
					<ownership>NoTransfer</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>TimeStamp</order>
					<semantics>Wind directory changes</semantics>
				</attribute>
                <attribute>
                    <name>Temperature</name>
                    <dataType>HLAinteger32BE</dataType>
                    <updateType>Conditional</updateType>
                    <updateCondition>Customer request</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAreliable</transportation>
                    <order>TimeStamp</order>
                    <semantics>Temperature of environment</semantics>
                </attribute>
                <attribute>
                    <name>Pressure</name>
                    <dataType>HLAinteger32BE</dataType>
                    <updateType>Conditional</updateType>
                    <updateCondition>Customer request</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAreliable</transportation>
                    <order>TimeStamp</order>
                    <semantics>Pressure of environment</semantics>
                </attribute>
			</objectClass>
			
			<objectClass>
				<name>Target</name>
				<sharing>PublishSubscribe</sharing>
				<semantics>Target to shoot</semantics>
				<attribute>
					<name>TargetID</name>
					<dataType>HLAinteger32BE</dataType>
					<updateType>Conditional</updateType>
					<updateCondition>Customer request</updateCondition>
					<ownership>NoTransfer</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>TimeStamp</order>
					<semantics>Id of a target</semantics>
				</attribute>
				<attribute>
					<name>Position</name>
					<dataType>Vector3</dataType>
					<updateType>Conditional</updateType>
					<updateCondition>Customer request</updateCondition>
					<ownership>NoTransfer</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>TimeStamp</order>
					<semantics>position of a target</semantics>
				</attribute>
			</objectClass>

			<objectClass>
				<name>Bullet</name>
				<sharing>PublishSubscribe</sharing>
				<semantics>Fired from the tank</semantics>
				<attribute>
					<name>BulletID</name>
					<dataType>HLAinteger32BE</dataType>
					<updateType>Conditional</updateType>
					<updateCondition>Customer request</updateCondition>
					<ownership>NoTransfer</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>TimeStamp</order>
					<semantics>Id of a bullet</semantics>
				</attribute>
				<attribute>
					<name>Position</name>
					<dataType>Vector3</dataType>
					<updateType>Conditional</updateType>
					<updateCondition>Customer request</updateCondition>
					<ownership>NoTransfer</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>TimeStamp</order>
					<semantics>position of a bullet</semantics>
				</attribute>
				<attribute>
					<name>Type</name>
					<dataType>BulletType</dataType>
					<updateType>Conditional</updateType>
					<updateCondition>Customer request</updateCondition>
					<ownership>NoTransfer</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>TimeStamp</order>
					<semantics>Whether the food was served in a reasonable amount of time</semantics>
				</attribute>
			</objectClass>
			
        </objectClass>
    </objects>
	
	<interactions>
        <interactionClass>
            <name>HLAinteractionRoot</name>
            <interactionClass>
				<name>Shot</name>
				<sharing>PublishSubscribe</sharing>
				<dimensions/>
				<transportation>HLAreliable</transportation>
				<order>TimeStamp</order>
				<semantics>Waiter has served entree</semantics>
				<parameter>
					<name>Position</name>
					<dataType>Vector3</dataType>
					<semantics>Whether the food was the correct temperature</semantics>
				</parameter>
				<parameter>
					<name>Direction</name>
					<dataType>Vector3</dataType>
					<semantics>Whether the correct food was served</semantics>
				</parameter>
				<parameter>
					<name>Type</name>
					<dataType>BulletType</dataType>
					<semantics>Whether the food was served in a reasonable amount of time</semantics>
				</parameter>
			</interactionClass>
			<interactionClass>
				<name>Hit</name>
				<sharing>PublishSubscribe</sharing>
				<dimensions/>
				<transportation>HLAreliable</transportation>
				<order>TimeStamp</order>
				<semantics>Waiter has served entree</semantics>
				<parameter>
					<name>TargetID</name>
					<dataType>HLAinteger32BE</dataType>
					<semantics>Whether the food was the correct temperature</semantics>
					<name>HitDirection</name>
					<dataType>Vector3</dataType>
					<semantics>Whether the food was the correct temperature</semantics>
				</parameter>
				<parameter>
					<name>Type</name>
					<dataType>BulletType</dataType>
					<semantics>Whether the food was served in a reasonable amount of time</semantics>
				</parameter>

			</interactionClass>
        </interactionClass>
    </interactions>
	
	<dataTypes>

        <enumeratedDataTypes>
            <enumeratedData>
                <name>BulletType</name>
                <representation>HLAinteger32BE</representation>
                <semantics>Possible types of bullets tank can shot</semantics>
                <enumerator>
                    <name>BlankShell</name>
                    <value>101</value>
                </enumerator>
                <enumerator>
                    <name>High-expplosive</name>
                    <value>102</value>
                </enumerator>
                <enumerator>
                    <name>ShrapnelShell</name>
                    <value>103</value>
                </enumerator>
                <enumerator>
                    <name>ClusterShell</name>
                    <value>104</value>
                </enumerator>
            </enumeratedData>
        </enumeratedDataTypes>
        <arrayDataTypes>
            <arrayData>
                <name>Vector3</name>
                <dataType>HLAfloat64BE</dataType>
                <cardinality>3</cardinality>
                <encoding>HLAfixedArray</encoding>
                <semantics>Shows</semantics>
            </arrayData>
        </arrayDataTypes>
        
    </dataTypes>
</objectModel>
